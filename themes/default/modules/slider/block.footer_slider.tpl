<!-- BEGIN: main -->
<div class="footer-gallery-wrapper boxed-style">
	<div class="container footer-gallery-container" id="footer-gallery-container">
		<div class="footer-gallery-nav-wrapper">
			<div class="footer-gallery-nav-left"></div>
			<div class="footer-gallery-title">{GLANG.partner}</div>
			<div class="footer-gallery-nav-right"></div>
			<div class="clear"></div>
		</div>
		<div class="gallery-item-holder row" id="gallery-item-holder" data-index="0">
			<!-- BEGIN: loop -->
			<div class="one-fifth column footer-gallery-item">
				<a href="{ROW.link_href}" title="{ROW.title}"{ROW.link_target}>
					<img src="{ROW.image}" alt="{ROW.image_alt}" />
				</a>
			</div>
			<!-- END: loop -->
			<div class="clear"></div>
		</div>
	</div>
</div>
<!-- END: main -->